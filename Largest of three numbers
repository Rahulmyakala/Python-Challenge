def Largest_of_all(a,b,c):
    if a>b and a>c:
        return a
    elif b>a and b>c:
        return b
    else:
        return c
number = int(input("Enter a number : "))
number1 = int(input("Enter a number : "))
number2 = int(input("Enter a number : "))
print("Largest number is : ",Largest_of_all(number,number1,number2))
